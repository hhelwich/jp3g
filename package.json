{
  "name": "@hhelwich/jpeg-js",
  "version": "0.0.0",
  "description": "JPEG encoding / decoding",
  "keywords": [
    "JPEG"
  ],
  "license": "MIT",
  "author": "Hendrik Helwich <hendrik@helwich.de>",
  "repository": "github:hhelwich/jpeg-js",
  "main": "dist/jp3g.min.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "format": "prettier --write .",
    "build": "del-cli dist && rollup -c && tsc -d --emitDeclarationOnly --resolveJsonModule --declarationDir dist --lib ES2019.Array,WebWorker src/index.ts && mv dist/src/ dist/types",
    "generate-test-images": "[ ! -d tmp ] && tsc --esModuleInterop --outDir tmp src/__tests__/util/generateTestImagesScript.ts && node tmp/__tests__/util/generateTestImagesScript.js && rm -r tmp"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/sharp": "^0.26.1",
    "canvas": "^2.6.1",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.34.0",
    "rollup-plugin-terser": "^7.0.2",
    "sharp": "^0.26.3",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
